{
  "expo": {
    "name": "Resistencia",
    "slug": "Resistencia",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/rpgIcon.png",
    "scheme": "resistencia",
    "plataforms": ["ios", "android", "web"],
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/rpgIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/rpgIcon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
